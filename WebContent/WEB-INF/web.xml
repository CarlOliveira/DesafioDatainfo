<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>DesafioDatainfo</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
        <servlet-name>MenuUsuarioTelefoneServlet</servlet-name>
        <servlet-class>servlets.MenuUsuarioTelefoneServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>MenuUsuarioTelefoneServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ValidaLoginServlet</servlet-name>
        <servlet-class>servlets.ValidaLoginServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ValidaLoginServlet</servlet-name>
        <url-pattern>/ValidaLoginServlet</url-pattern>
    </servlet-mapping>
   
</web-app>